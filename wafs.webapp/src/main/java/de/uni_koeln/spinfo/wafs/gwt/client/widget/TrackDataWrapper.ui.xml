<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:com.github.gwtbootstrap.client.ui">
	<g:HTMLPanel>
		<p>
			<b:Image ui:field="image" />
			<br />
			<span class='text-info'>Album:</span>
			<span class='muted' ui:field="album"></span>
			<br />
			<span class='text-info'>Artist:</span>
			<span class='muted' ui:field="artist"></span>
			<br />
			<span class='text-info'>Title:</span>
			<span class='muted' ui:field="tracktitle"></span>
			<br />
			<span class='text-info'>BitRate:</span>
			<span class='muted' ui:field="bitrate"></span>
		</p>
	</g:HTMLPanel>
</ui:UiBinder> 